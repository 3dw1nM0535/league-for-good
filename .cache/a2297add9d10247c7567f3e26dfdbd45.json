te_league/CreateLeagueForm.jsx","original":{"line":96,"column":1}},{"name":"mapFormStateToProps","generated":{"line":161,"column":25},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":96,"column":9}},{"generated":{"line":161,"column":44},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":96,"column":1}},{"name":"mapDispatchToProps","generated":{"line":161,"column":46},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":96,"column":30}},{"generated":{"line":161,"column":64},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":96,"column":1}},{"generated":{"line":162,"column":0},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":97,"column":2}},{"generated":{"line":163,"column":0},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":2}},{"generated":{"line":163,"column":26},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":12}},{"name":"form","generated":{"line":163,"column":28},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":14}},{"generated":{"line":163,"column":34},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":20}},{"generated":{"line":163,"column":52},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":12}},{"name":"validate","generated":{"line":163,"column":54},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":40}},{"generated":{"line":163,"column":82},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":12}},{"generated":{"line":163,"column":84},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":2}},{"name":"CreateLeagueForm","generated":{"line":163,"column":86},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":99,"column":3}},{"generated":{"line":163,"column":102},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":98,"column":2}},{"generated":{"line":163,"column":103},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":96,"column":1}},{"generated":{"line":163,"column":104},"source":"src/components/create_league/CreateLeagueForm.jsx","original":{"line":94,"column":15}}],"sources":{"src/components/create_league/CreateLeagueForm.jsx":"import React, { Component } from 'react';\nimport PropTypes from 'prop-types';\nimport { withRouter } from 'react-router-dom';\nimport { TextField } from 'redux-form-material-ui';\nimport RaisedButton from 'material-ui/RaisedButton';\nimport { Field, reduxForm, formValueSelector } from 'redux-form';\nimport { bindActionCreators } from 'redux';\nimport { connect } from 'react-redux';\n\nimport validate from './validate';\nimport CreateLeagueSelector from './CreateLeagueSelector.jsx';\nimport { createLeague } from '../../actions/index';\nimport { cssContent, cssCreateLeague } from '../styles';\n\n\nclass CreateLeagueForm extends Component {\n\n\tonSubmit = formBody => {\n\t\tconst { createLeague, history } = this.props;\n\t\tconst redirectMethod = () => history.push('/');\n\n\t\t// call the createLeague action and pass in the validated form fields\n\t\t// and a callback function to redirect the url.\n\t\t// I cant figure out another way to trigger the redirect after\n\t\t// a successful form submission than by accessing react-routers\n\t\t// built in method from inside the component\n\t\tcreateLeague(formBody, redirectMethod);\n\t}\n\n\trender() {\n\t\tconst { handleSubmit, change, SelectedSportType } = this.props;\n\n\t\treturn (\n\t\t\t<div style={cssContent.header}>\n\t\t\t\t<CreateLeagueSelector\n\t\t\t\t\tonSelect={(sport) => change('sportType', sport)}\n\t\t\t\t\tselectedSport={SelectedSportType}\n\t\t\t\t/>\n\t\t\t\t{\n\t\t\t\t\tSelectedSportType &&\n\t\t\t\t\t<div style={cssContent.body}>\n\t\t\t\t\t\t<form\n\t\t\t\t\t\t\tonSubmit={ handleSubmit(this.onSubmit)}\n\t\t\t\t\t\t\tstyle={cssCreateLeague.form}\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t<Field\n\t\t\t\t\t\t\t\tcomponent='input'\n\t\t\t\t\t\t\t\tname='sportType'\n\t\t\t\t\t\t\t\ttype='hidden'\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<Field\n\t\t\t\t\t\t\t\tcomponent={TextField}\n\t\t\t\t\t\t\t\tfloatingLabelText={`${SelectedSportType} League Name`}\n\t\t\t\t\t\t\t\tfullWidth={true}\n\t\t\t\t\t\t\t\tname='name'\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<RaisedButton\n\t\t\t\t\t\t\t\tbackgroundColor={cssCreateLeague.raisedButton.backgroundColor}\n\t\t\t\t\t\t\t\tlabel='Create'\n\t\t\t\t\t\t\t\tlabelStyle={cssCreateLeague.raisedButton.label}\n\t\t\t\t\t\t\t\tstyle={cssCreateLeague.raisedButton.style}\n\t\t\t\t\t\t\t\ttype='submit'\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</form>\n\t\t\t\t\t</div>\n\t\t\t\t}\n\t\t\t</div>\n\t\t);\n\t}\n}\n\nCreateLeagueForm.propTypes = {\n\tSelectedSportType: PropTypes.string,\n\tchange: PropTypes.func,\n\tcreateLeague: PropTypes.func,\n\thandleSubmit: PropTypes.func,\n\thistory: PropTypes.object\n};\n\n// redux-form method to access form field values\nconst selector = formValueSelector('CreateLeagueForm');\n\n// Callback function passed to the connect function to access the form state\nfunction mapFormStateToProps(state) {\n\treturn {SelectedSportType: selector(state, 'sportType')};\n}\n\nfunction mapDispatchToProps(dispatch) {\n\treturn bindActionCreators({ createLeague }, dispatch);\n}\n\n// Decorate component one last time with react-router bindings in order\n// to redirect user after a successful form submission\nexport default withRouter(\n\t// Decorate component with redux bindings\n\tconnect(mapFormStateToProps, mapDispatchToProps)(\n\t\t// Decorate component with redux-form\n\t\treduxForm({ form: 'CreateLeagueForm', validate })(\n\t\t\tCreateLeagueForm\n\t\t)\n\t)\n);\n"},"lineCount":null}},"hash":"dec3fc20855cd7f9335313461cd1b762","cacheData":{"env":{}}}