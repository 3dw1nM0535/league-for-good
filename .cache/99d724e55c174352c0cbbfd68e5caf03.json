d as unix timestamps\nconst format = v =>\n\ttypeof v === 'number' ? new Date(v) : v;\n\n\nconst EditSeasonForm = () => {\n\n\treturn (\n\t\t<form style={cssDashboard.form}>\n\t\t\t<div style={cssDashboard.formRow}>\n\t\t\t\t<Field\n\t\t\t\t\tautoOk={true}\n\t\t\t\t\tcomponent={DatePicker}\n\t\t\t\t\tfloatingLabelStyle={cssDashboard.formRequired}\n\t\t\t\t\tfloatingLabelText='Start Date'\n\t\t\t\t\tformat={format}\n\t\t\t\t\tformatDate={getFormattedDate}\n\t\t\t\t\tname='startDate'\n\t\t\t\t/>\n\t\t\t\t<Field\n\t\t\t\t\tautoOk={true}\n\t\t\t\t\tcomponent={DatePicker}\n\t\t\t\t\tfloatingLabelStyle={cssDashboard.formRequired}\n\t\t\t\t\tfloatingLabelText='End Date'\n\t\t\t\t\tformat={format}\n\t\t\t\t\tformatDate={getFormattedDate}\n\t\t\t\t\tname='endDate'\n\t\t\t\t/>\n\t\t\t</div>\n\t\t\t<br/>\n\t\t\t<div style={cssDashboard.formRow}>\n\t\t\t\t<Field\n\t\t\t\t\tcomponent={TextField}\n\t\t\t\t\tfloatingLabelStyle={cssDashboard.formRequired}\n\t\t\t\t\tfloatingLabelText='Season name*'\n\t\t\t\t\thintText='Name of the season'\n\t\t\t\t\tname='name'\n\t\t\t\t/>\n\t\t\t</div>\n\t\t</form>\n\t);\n};\n\n\nexport default reduxForm({\n\tform: 'editSeasonForm',\n\tonSubmit: editSeason,\n\tonSubmitSuccess: openSnackbar,\n\tvalidate\n})(EditSeasonForm);\n\n\n"},"lineCount":null}},"hash":"f8a4f567213f32154bf5055fe981a649","cacheData":{"env":{}}}